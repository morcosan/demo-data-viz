{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.18.0"
  },
  "scripts": {
    "app--dev": "concurrently \"node tooling/run-dev.ts\" \"next dev\"",
    "app--build": "node tooling/run-build.ts && next build",
    "app--serve": "concurrently \"node tooling/run-dev.ts\" \"vite preview --outDir out --port 3000\"",
    "sb--dev": "storybook dev -p 6006 --no-open",
    "sb--build": "storybook build -o out-sb",
    "sb--serve": "vite preview --outDir out-sb --port 6006",
    "sb--test": "vitest --project=storybook",
    "lint": "eslint .",
    "clear-cache": "rmdir /s /q .next"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@mantine/core": "^8.3.15",
    "@tanstack/react-query": "^5.90.21",
    "@tanstack/react-table": "^8.21.3",
    "@tanstack/react-virtual": "^3.13.19",
    "clsx": "^2.1.1",
    "flag-icons": "^7.5.0",
    "i18n-iso-countries": "^7.14.0",
    "i18next": "^25.8.13",
    "lodash": "^4.17.23",
    "next": "16.1.6",
    "react": "19.2.4",
    "react-dom": "19.2.4",
    "react-error-boundary": "^6.1.1",
    "react-i18next": "^16.5.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.3.0",
    "@storybook/addon-a11y": "^10.2.13",
    "@storybook/addon-docs": "^10.2.13",
    "@storybook/addon-vitest": "^10.2.13",
    "@storybook/nextjs-vite": "^10.2.13",
    "@storybook/react": "^10.2.13",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.2.1",
    "@types/node": "^25.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "@vitest/browser-playwright": "^4.0.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.3",
    "eslint-config-next": "16.1.6",
    "eslint-plugin-storybook": "^10.2.13",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "prettier-plugin-css-order": "^2.2.0",
    "prettier-plugin-organize-imports": "^4.3.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "remark-gfm": "^4.0.1",
    "storybook": "^10.2.13",
    "tailwindcss": "^4.2.1",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^4.0.18"
  },
  "prettier": "../.config/prettier.config.js"
}
